{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "clean bin",
            "type": "shell",
            "group": "build",
            "command":"rm",
            "args": ["bin\\*"]
        },
        {
            "type": "go",
            "label": "build default",
            "command": "build",
			"args": [
				"-o",
				"..\\bin\\tilinghelper-${config:from_branch}_${config:to_branch}.exe"
			],
			"options": {
				"cwd": "${workspaceFolder}\\src"
			},
            "group": "build",
            "dependsOn":[
                "clean bin"
            ]
        }
    ]
}
